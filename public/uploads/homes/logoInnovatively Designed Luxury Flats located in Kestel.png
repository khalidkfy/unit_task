var cookieList = function(cookieName) {		
	
	var cookie = $.cookie(cookieName);		
	
	if(cookie){			
		var items =   JSON.parse(cookie);			
		}else{			
		var items =  new Array();			
	}		
	
	return {			
		"add": function(val) {				
			items.push(val);				
			$.cookie(cookieName, JSON.stringify(items));				
		},			
		"remove": function (val) {				
			
			items = jQuery.grep(items, function(value) {					
				return value != val;					
			});				
			
			$.cookie(cookieName, JSON.stringify(items));    				
		},			
		"clear": function() {				
			items = null;				
			
			$.cookie(cookieName, null);				
		},			
		"items": function() {				
			
			return items;				
		}			
	}		
}  	


